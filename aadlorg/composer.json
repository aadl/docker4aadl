    "require": {
        "php": ">=7.0.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.8.0",
        "drupal/core-composer-scaffold": "^8.8.0",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0",

        "aadl/aadl-content-management": "dev-master",
        "aadl/aadlfreespace": "dev-master",
        "aadl/accountfix": "dev-master",
        "aadl/arborcat": "dev-master",
        "aadl/arborelastic": "dev-master",
        "aadl/askus": "dev-master",
        "aadl/bisac-assign": "dev-master",
        "aadl/ccollections": "dev-master",
        "aadl/events": "dev-master",
        "aadl/ezproxy": "dev-8.x-1.x",
        "aadl/fesliders": "dev-master",
        "aadl/ill": "dev-master",
        "aadl/itsallwrite": "dev-master",
        "aadl/newslater": "dev-master",
        "aadl/oldnews": "dev-master",
        "aadl/patron_booking": "dev-master",
        "aadl/patron_comments": "dev-master",
        "aadl/pointsomatic": "dev-master",
        "aadl/square": "dev-master",
        "aadl/statusinator": "dev-master",
        "aadl/summergame": "dev-master",
        "aadl/twilio": "dev-master",
        "aadl/uc_summergame": "dev-master",
        "aadl/ums_cardfile": "dev-drupal8migration",
        "aadl/toolbooking": "dev-drupalDBTableAdditions2",
        "aadl/aadl.org-frontend": "dev-tool-booking-UI"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },

    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::setupAADLTheme"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "DrupalProject\\composer\\ScriptHandler::setupAADLTheme"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
